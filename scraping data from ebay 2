    "prod_urls = soup.find_all(\"a\", class_=\"s-item__link\")\n",
    "url_list = []\n",
    "for link in prod_urls:\n",
    "    url = link.get(\"href\")\n",
    "    url_list.append(url)\n",
    "    \n",
    "    \n",
    "for product_page in url_list:\n",
    "        product_response = requests.get(product_page)\n",
    "\n",
    "        if product_response.status_code == 200:\n",
    "            product_soup = BeautifulSoup(product_response.content, 'html.parser')\n",
    "\n",
    "            \n",
    "            product_name = product_soup.find('span', class_='ux-textspans ux-textspans--BOLD').text.strip()\n",
    "            return_policy = product_soup.find('div', class_='vim x-returns-minview').text.strip()\n",
    "            ships_to = product_soup.find('div', class_='ux-labels-values col-12 ux-labels-values--shipsto').text.strip()\n",
    "            item_location = product_soup.find('div', class_='ux-labels-values-with-hints ux-labels-values-with-hints--NO-SPACE ux-labels-values-with-hints--SECONDARY-SMALL').text.strip()\n",
    "            shipping_cost = product_soup.find('td', class_='ux-table-section__cell').text.strip()\n",
    "            edd = product_soup.find('div', class_='ux-expandable-textual-display-block__item')\n",
    "            payment_modes = product_soup.find('div', class_='ux-table-section__cell')\n",
    "            price = product_soup.find('div', class_='x-price-primary').text.strip()\n",
    "            starting_bid = product_soup.find('div', class_='x-bid-price__content')\n",
    "            item_no = product_soup.find('div', class_='ux-layout-section__textual-display ux-layout-section__textual-display--itemId').text.strip()\n",
    "            Condition = product_soup.find('div', class_='x-item-condition-text').text.strip()\n",
    "           \n",
    "            Brand_tag = product_soup.find('span', text='Brand')\n",
    "            Brand=Brand_tag.find_next('span').get_text() if Brand_tag else \"NaN\"\n",
    "            \n",
    "            color = product_soup.find('span', text='Color')\n",
    "            color_tag=color.find_next('span') if color else None\n",
    "            color_text=color_tag.get_text()if color_tag else \"NaN\"\n",
    "            \n",
    "            types = product_soup.find('span', text='Type')\n",
    "            type_tag=types.find_next('span') if types else None\n",
    "            type_text=type_tag.get_text()if type_tag else \"NaN\"\n",
    "            \n",
    "            \n",
    "            material = product_soup.find('span', text='Material')\n",
    "            material_tag=material.find_next('span') if material else None\n",
    "            material_text=material_tag.get_text()if material_tag else \"NaN\"\n",
    "            \n",
    "            details_df = details_df.append({'Product Name': product_name, \n",
    "                                            'Return Policy': return_policy,\n",
    "                                            'Ships to': ships_to,\n",
